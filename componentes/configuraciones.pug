-var titulo = "sub-titulo Principal"
-var titulos = ["tutulo Principal", "Subtitulo 1", "Subtitulo 2", "Subtitulo 3"]
-var enlaces=['Acerca', 'Noticias', 'Contacto']
mixin caja(imagen,titulo,contenido)
    .caja
        .caja__imagen: img(src="images/"+imagen)
        .caja__contenido
        h3=titulo
        p=contenido